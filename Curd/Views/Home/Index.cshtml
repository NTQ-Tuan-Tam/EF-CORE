@using static Curd.Controllers.HomeController
@{
    ViewData["Title"] = "Home Page";
    //optison 1 inra dux lieu bang viewbag
   // var students = ViewBag.student as List<Curd.Controllers.Student>; 
    //option in ra du lieu bang viewdata
    var students = ViewData["Students"] as List<Student>;
}


<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
<div>
    <table class="table">
        <tr >
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
        </tr>

        @foreach(var student in students)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
            </tr>
        }
    </table>
</div>
